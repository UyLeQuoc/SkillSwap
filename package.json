{
  "name": "skillswap",
  "version": "1.0.0",
  "description": "Using NextJs 15 and NestJS with Graphql, Postgres and Shadcn. With CI CD",
  "keywords": [
    "skillswap",
    "nextjs15",
    "nestjs"
  ],
  "homepage": "https://github.com/UyLeQuoc/skillswap#readme",
  "bugs": {
    "url": "https://github.com/UyLeQuoc/skillswap/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UyLeQuoc/skillswap.git"
  },
  "license": "ISC",
  "author": "UyDev",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"yarn:dev:api\" \"yarn:dev:web\"",
    "setup": "yarn install && cd apps/api && yarn install && cd ../.. && cd apps/web && yarn install",
    "dev:api": "cd apps/api && yarn start:dev",
    "dev:web": "cd apps/web && yarn dev",
    "gen": "cd apps/web && yarn gen",
    "generate": "cd apps/api && yarn generate",
    "migrate": "cd apps/api && yarn migrate",
    "seed": "cd apps/api && yarn seed",
    "reset": "cd apps/api && yarn reset"
  },
  "dependencies": {
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "concurrently": "^9.1.2"
  }
}
